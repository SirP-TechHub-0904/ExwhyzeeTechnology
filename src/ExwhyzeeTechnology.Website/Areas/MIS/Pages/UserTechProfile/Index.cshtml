@page
@model ExwhyzeeTechnology.Website.Areas.MIS.Pages.UserTechProfile.IndexModel
@{
}

<div class="container-fluid">
    <button type="button" class="btn btn-block btn-primary btn-xs" style="margin:10px 0;">
        <h3>My Profile</h3>
    </button>
    <button type="button" class="btn btn-block btn-danger btn-xs" style="margin:10px 0;">
        <h3>You have not been admitted yet.</h3>
    </button>
    <div class="row">
        <div class="col-md-3">

            <div class="card card-primary card-outline">
                <div class="card-body box-profile">
                    @*<div class="text-center">
                        <img class="profile-user-img img-fluid img-circle" src="@Model.UserDatas.PassportFilePathUrl" alt="User profile picture">
                    </div>*@
                    <h3 class="profile-username text-center">@Model.UserDatas.FullnameX</h3>

                </div>

            </div>


            <div class="card card-primary">
                <div class="card-header">
                    <h3 class="card-title">About Me</h3>
                </div>



            </div>

        </div>

        <div class="col-md-9">
            <div class="card">
                <div class="card-header p-2">
                    <ul class="nav nav-pills">
                        <li class="nav-item"><a class="nav-link active" href="#activity" data-toggle="tab">Activity</a></li>
                        <li class="nav-item"><a class="nav-link" href="#timeline" data-toggle="tab">Timeline</a></li>
                        <li class="nav-item"><a class="nav-link" href="#settings" data-toggle="tab">Settings</a></li>
                    </ul>
                </div>
                <div class="card-body">
                    <div class="tab-content">
                        <div class="active tab-pane" id="activity">

                            <h3>No activity</h3>
                        </div>

                        <div class="tab-pane" id="timeline">

                            <h3>No activity</h3>
                        </div>

                        <div class="tab-pane" id="settings">
                            <h3>No activity</h3>
                        </div>

                    </div>

                </div>
            </div>

        </div>

    </div>

</div>