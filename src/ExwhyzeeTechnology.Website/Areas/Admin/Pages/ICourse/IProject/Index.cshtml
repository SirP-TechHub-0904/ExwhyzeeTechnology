@page
@model ExwhyzeeTechnology.Website.Areas.Admin.Pages.ICourse.IProject.IndexModel

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-page="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.CohortProject[0].Title)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CohortProject[0].Question)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CohortProject[0].FIleUrl)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CohortProject[0].FIleKey)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CohortProject[0].Date)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CohortProject[0].LastSubmissionDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CohortProject[0].Cohort)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CohortProject[0].User)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.CohortProject) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Title)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Question)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FIleUrl)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FIleKey)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Date)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LastSubmissionDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Cohort.CohortCode)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.User.Id)
            </td>
            <td>
                <a asp-page="./Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-page="./Details" asp-route-id="@item.Id">Details</a> |
                <a asp-page="./Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
